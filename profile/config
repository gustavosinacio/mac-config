#!/bin/bash

CONFIG_FOLDER_NAME="profile"
CONFIG_FILE_NAME=".profile"

CONFIG_FOLDER_NAME_LOCAL="profile"
CONFIG_FILE_NAME_LOCAL=".local_profile"

. $MAC_CONFIG_HOME/general-config $CONFIG_FOLDER_NAME $CONFIG_FILE_NAME
. $MAC_CONFIG_HOME/general-config $CONFIG_FOLDER_NAME_LOCAL $CONFIG_FILE_NAME_LOCAL
